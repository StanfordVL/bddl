        (ontop ski.n.01_2 floor.n.01_1) 
        (ontop bicycle.n.01_1 floor.n.01_1) 
        (ontop tennis_racket.n.01_1 floor.n.01_1) 
        (ontop hockey_stick.n.01_1 floor.n.01_1) 
