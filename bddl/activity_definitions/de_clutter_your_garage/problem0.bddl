        (inroom bookcase.n.01_1 garage) 
        (inroom floor.n.01_1 garage) 
        (inroom door.n.01_1 garage)
    )
    
    (:goal 
        (and 
            (forall 
                (?ski.n.01 - ski.n.01) 
                (ontop ?ski.n.01 ?bookcase.n.01_1)
            ) 
            (nextto ?bicycle.n.01_1 ?door.n.01_1) 
            (nextto ?carton.n.02_1 ?door.n.01_1) 
            (inside ?tennis_racket.n.01_1 ?bookcase.n.01_1)
            (inside ?hockey_stick.n.01_1 ?bookcase.n.01_1)
            (inside ?shovel.n.01_1 ?cabinet.n.01_1)
