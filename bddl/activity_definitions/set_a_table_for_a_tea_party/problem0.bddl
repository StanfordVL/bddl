        (ontop danish.n.02_1 tray.n.01_1) 
        (ontop danish.n.02_2 tray.n.01_1) 
        (ontop scone.n.01_1 tray.n.01_1) 
        (ontop sugar_cookie.n.01_1 tray.n.01_1) 
